<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration />
    <LastUsedPlatform />
    <SiteUrlToLaunchAfterPublish>http://democles-api.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>True</ExcludeApp_Data>
    <MSDeployServiceURL>democles-api.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>democles-api</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>False</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$democles-api</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="damoclesConnection" Order="1" Enabled="True">
          <Destination Path="Data Source=tcp:yattaoa2kz.database.windows.net,1433;Initial Catalog=damocles;Integrated Security=False;User ID=hetu-dbadmin@yattaoa2kz;Password=!@admin123;Connect Timeout=30;Encrypt=True" Name="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="Api.Models.ApplicationDbContext, Api" MigrationConfiguration="Api.Migrations.Configuration, Api" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)damoclesConnection-Web.config Connection String">
      <ParameterValue>Data Source=tcp:yattaoa2kz.database.windows.net,1433;Initial Catalog=damocles;Integrated Security=False;User ID=hetu-dbadmin@yattaoa2kz;Password=!@admin123;Connect Timeout=30;Encrypt=True</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>